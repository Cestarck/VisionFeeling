<h1> Users </h1>
<table class="table table-sm">
  <thead class="thead-dark">
    <tr>
      <th scope="col">email</th>
      <th scope="col">role</th>
      <th scope="col">status</th>
      <th scope="col">updated_at</th>
      <th>DETALLES</th>
    </tr>
  </thead>
  <tbody>
    {{#each users as | user | }}
      {{#if user.status}}
        <tr>
      {{else}}
        <tr class="table-danger">
      {{/if}}
        <td>{{ user.email }}</td>
        <td>{{ user.role }}</td>
        <td>{{ user.status }}</td>
        <td>{{ user.updated_at }}</td>
        <th scope="row"><a href="user-list-profile/{{user._id}}">{{user.username}}</a></th>
      </tr>
    {{/each}}
  </tbody>
</table>
<br>
<a href="/">Return</a>
